{
    "name": "arduino",
    "displayName": "Arduino",
    "version": "0.0.5",
    "publisher": "moozzyk",
    "description": "Arduino support for Visual Studio Code",
    "author": {
        "name": "Pawel Kadluczka"
    },
    "icon": "images/ArduinoCommunityLogo.png",
    "categories": [
        "Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/moozzyk/ArduinoCode.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "contributes": {
        "languages": [{
            "id": "arduino",
            "extensions": [ ".ino", ".pde" ],
            "aliases": [ "Arduino", "arduino" ],
            "configuration": "./arduino.configuration.json"
        }],
        "grammars": [{
            "language": "arduino",
            "path": "./syntaxes/arduino.tmLanguage",
            "scopeName": "source.cpp.arduino"
        }],
        "snippets": [
            {
                "language": "arduino",
                "path": "./snippets/arduino.json"
            }
        ]
    }
}
